{
  "name": "eslint-config-airlight-base",
  "version": "7.7.2",
  "description": "ESLint configure base focused on code quality, security and performance on top of eslint-config-airbnb-typescript/base",
  "type": "module",
  "main": "flat.js",
  "exports": {
    ".": "./flat.js",
    "./flat": "./flat.js",
    "./legacy": "./.eslintrc.json",
    "./oxlintrc.json": "./oxlintrc.json",
    "./biome.json": "./biome.json",
    "./rules.json": "./rules.json",
    "./settings.json": "./settings.json",
    "./overrides/*": "./overrides/*"
  },
  "files": [
    "plugins",
    "overrides",
    "airbnb-rules.json",
    "rules.json",
    "flat.js",
    ".eslintrc.json",
    "settings.json",
    "oxlintrc.json",
    "biome.json"
  ],
  "scripts": {
    "prepublishOnly": "node scripts/*.js && biome migrate eslint --write --include-inspired --include-nursery && biome format biome.json --write"
  },
  "engines": {
    "node": ">=20.19.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalisoft/airlight.git"
  },
  "keywords": [
    "eslint",
    "lint",
    "config",
    "airbnb",
    "node"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dalisoft/airlight/issues"
  },
  "homepage": "https://github.com/dalisoft/airlight#readme",
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.37.0",
    "@stylistic/eslint-plugin": "^5.3.x",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-optimize-regex": "https://github.com/aminya/eslint-plugin-optimize-regex.git#eslint9",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.4.0",
    "typescript-eslint": "^8.46.0"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "^6.11.3"
  },
  "peerDependencies": {
    "eslint": "^8.57.x || ^9.35.x",
    "prettier": "^3.6.x",
    "typescript": "^5.9.x"
  }
}
